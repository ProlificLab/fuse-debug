{
	"version": "0.2.0",
	"configurations": [				
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"program": "${workspaceRoot}/fuse.js",
			"showAsyncStacks": true,
			"smartStep": true
		},
		{
			"type": "chrome",
			"request": "launch",
			"name": "Launch Chrome",
			"url": "http://localhost:4444",
			"webRoot": "${workspaceRoot}",
			"showAsyncStacks": true,
			"disableNetworkCache": true
		}		
	],
	"compounds": [
		{
			"name": "Start Both",
			"configurations": [
				"Launch Program",
				"Launch Chrome"
			]
		}
	]
}